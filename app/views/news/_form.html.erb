        <div class="form-group">
            <label>Title</label>
            <%= f.text_field :title, class: 'form-control', placeholder: 'Title' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Author</label>
            <%= f.text_field :author, class: 'form-control', placeholder: 'Author' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Photographer</label>
            <%= f.text_field :photographer, class: 'form-control', placeholder: 'Photographer' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Category</label>
            <%= f.select :category_id, category_options, {}, class: 'form-control' %>
        </div>
        <div class="form-group">
            <label>Date</label>
            <%= f.date_field :date, class: 'form-control' %>
        </div>        
        <div class="form-group">
            <label>Image URL</label>
            <%= f.text_field :image_url, class: 'form-control', placeholder: 'Image URL' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Description</label>
            <%= f.text_field :description, class: 'form-control', placeholder: 'Description' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Keyword</label>
            <%= f.text_field :keyword, class: 'form-control', placeholder: 'Keyword' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Content</label>
            <%= f.text_area :content, class: 'form-control', placeholder: 'Content' %>
            <!--p class="help-block">Example block-level help text here.</p-->
        </div>
        <div class="form-group">
            <label>Publish Date</label>
            <%= f.date_field :publish_at, class: 'form-control' %>
        </div>        
        <div class="form-group">
            <label>Retract Date</label>
            <%= f.date_field :retract_at, class: 'form-control' %>
        </div>        
        <div class="form-group">
            <label>Image Upload</label>
            <%= f.file_field :image, as: :file, class: 'form-control' %>
        </div>        
        <% unless @news.new_record? || !@news.image.exists? %>
        <div class="form-group">
            <label>Image Tag</label>
            <%= image_tag @news.image.url(:thumb) %>
        </div>
        <div class="form-group">
            <label>Image Tag</label>
            <%= image_tag @news.image.url(:medium) %>
        </div>
        <div class="form-group">
            <label>Image Tag</label>
            <%= image_tag @news.image.url %>
        </div>
        <% end %>
